{
  "name": "wm2",
  "description": "Git worktree management tool for easy workspace organization",
  "version": "0.0.1",
  "author": "nacyot <propellerheaven@gmail.com>",
  "bin": {
    "wm2": "./bin/run.js"
  },
  "bugs": "https://github.com/nacyot/worktree_manager/issues",
  "dependencies": {
    "@inquirer/prompts": "^7.7.1",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "execa": "^9.6.0",
    "inquirer": "^12.8.2",
    "js-yaml": "^4.1.0",
    "simple-git": "^3.28.0"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/js-yaml": "^4.0.9",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./.worktree.yml.example",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/nacyot/worktree_manager",
  "keywords": [
    "git",
    "worktree",
    "workspace",
    "cli",
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "wm2",
    "dirname": "wm2",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " "
  },
  "repository": "nacyot/worktree_manager",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "vitest run",
    "test:watch": "vitest",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
